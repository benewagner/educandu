<div class="PageHeader">
  <a class="PageHeader-logo" href="/">elmu</a>
  <a class="PageHeader-editLink" href="/edit/doc/<%= doc._id %>">Bearbeiten</a>
</div>
<main>
  <% doc.sections.forEach(section => { %>
  <section
    class="Section"
    data-section-id="<%= section._id %>"
    data-section-order="<%= section.order %>"
    data-section-type="<%= section.type %>"
    >
    <%- section._rendered.de %>
  </section>
  <% }); %>
</main>
<script>
  window.__initalState__ = <%- htmlescape(doc) %>;
</script>
<script src="/doc.js"></script>
